<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="wetalk.software.bupt.com.wetalk.view.activity.NewFriendsActivity">

    <include layout="@layout/layout_title"/>
    <wetalk.software.bupt.com.wetalk.widget.ClearEditText
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:id="@+id/et_friends_search"
        android:layout_width="match_parent"
        android:layout_height="@dimen/base_action_bar_height"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:background="@drawable/base_edit_input"
        android:drawableLeft="@drawable/icon_msg_search"
        android:drawablePadding="10dp"
        android:hint="账号/手机号"
        android:focusable="false"
        android:focusableInTouchMode="false"

        />


</LinearLayout>
